{
  "manifest_version": 3,
  "name": "ChatGPT Starred Chats",
  "version": "1.0",
  "description": "Add star markers next to ChatGPT stars",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["extension/content.js"],
      "css": ["extension/styles.css"]
    }
  ],
  "icons": {
    "16": "images/star_icon_img.png",
    "32": "images/star_icon_img.png",
    "48": "images/star_icon_img.png"
  }
}
